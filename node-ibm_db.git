  ;

<span class="pl-smi">ibmdb</span>.<span class="pl-c1">open</span>(cn, <span class="pl-k">function</span>(<span class="pl-smi">err</span>,<span class="pl-smi">conn</span>) {

  <span class="pl-smi">conn</span>.<span class="pl-en">beginTransaction</span>(<span class="pl-k">function</span> (<span class="pl-smi">err</span>) {
    <span class="pl-k">if</span> (err) {
      <span class="pl-c">//could not begin a transaction for some reason.</span>
      <span class="pl-en">console</span>.<span class="pl-c1">log</span>(err);
      <span class="pl-k">return</span> <span class="pl-smi">conn</span>.<span class="pl-en">closeSync</span>();
    }

    <span class="pl-k">var</span> result <span class="pl-k">=</span> <span class="pl-smi">conn</span>.<span class="pl-en">querySync</span>(<span class="pl-s"><span class="pl-pds">"</span>insert into customer (customerCode) values ('stevedave')<span class="pl-pds">"</span></span>);

    <span class="pl-smi">conn</span>.<span class="pl-en">rollbackTransaction</span>(<span class="pl-k">function</span> (<span class="pl-smi">err</span>) {
      <span class="pl-k">if</span> (err) {
        <span class="pl-c">//error during rollback</span>
        <span class="pl-en">console</span>.<span class="pl-c1">log</span>(err);
        <span class="pl-k">return</span> <span class="pl-smi">conn</span>.<span class="pl-en">closeSync</span>();
      }

    <span class="pl-en">console</span>.<span class="pl-c1">log</span>(<span class="pl-smi">conn</span>.<span class="pl-en">querySync</span>(<span class="pl-s"><span class="pl-pds">"</span>select * from customer where customerCode = 'stevedave'<span class="pl-pds">"</span></span>));

     <span class="pl-c">//Close the connection</span>
     <span class="pl-smi">conn</span>.<span class="pl-en">closeSync</span>();
    });
  });
});</pre></div>

